</Group><CodeEditor-ExecGroup view="presentation" hide-input="false" labelreference="L648" drawlabel="true" applyint="true" applyrational="true" display="code"><EC-CodeEditor id="CodeEditRegion6" expanded="true" visible="true" pixel-width="500" pixel-height="200" code-language="text/maple" autofit="true" wrapping="true" show-border="true" code-line-numbers="true">####Create ODEs####
MatrixReader := proc(L,W)
local a,S,k,l,b,s,i,j,h,hh,checker,N,OUTPUT,Subber1,Subber2,OutputList,ICs,BiList,up,down,left,right,MatList,NameList;
BiList:=BinaryList(L,W)[1];
OutputList:=BinaryList(L,W)[1];
N:=nops(OutputList);
OUTPUT:=[];

###Remove symmetries from possible candidates###
for h from 1 to N do
		MatList:=List2Mat(OutputList[h],L,W);
		NameList:=List2Name(OutputList[h]);

		for hh from 1 to L do
		MatList:=MatList[[2..-1,1]][1..-1];
		Subber2:=Mat2List(MatList)=OutputList[h];
		OutputList:=subs(Subber2,OutputList):
		
		Subber2:=Mat2List(FlipDimension(MatList,1))=OutputList[h];
		OutputList:=subs(Subber2,OutputList):
		end do:
end do;
OutputList:=MakeUnique(OutputList);
N:=nops(OutputList);

###Create ODEs for reduced candidates###
for h from 1 to N do
	S:=0;
	a:=List2Mat(OutputList[h],L,W);

	for i from 1 to W do
		for j from 1 to L do
			up:=j-1; if up=0 then up:=L; end if;
			down:=j+1; if down=L+1 then down:=1; end if;
			left:=i-1;
			right:=i+1;
			
			if left=0 then
				s:=a[up,i]+a[down,i]+a[j,right];
			elif right=W+1 then
				s:=a[up,i]+a[down,i]+a[j,left];
			else
				s:=a[up,i]+a[down,i]+a[j,left]+a[j,right];
			end if;
	
			b:=a;
	          b[j,i]:=0;
			a:=List2Mat(OutputList[h],L,W);
	
			checker:=1;
			if not(member(Mat2List(b),BiList)) then
				checker:=0;
			end if:
		
		
			S:=S+s*(a[j,i]*checker*P[convert(Transpose(b),list)](t)-(1-a[j,i])*P[convert(Transpose(a),list)](t)):
			end do;
	end do;
	OUTPUT:=[op(OUTPUT),diff(P[convert(Transpose(a),list)](t),t)=S];
end do;

###Remove symmetries from ODEs###
for h from 1 to N do
		MatList:=List2Mat(OutputList[h],L,W);
		NameList:=List2Name(OutputList[h]);

		for hh from 1 to L do
		MatList:=MatList[[2..-1,1]][1..-1];
		Subber1:=P[List2Name(Mat2List(MatList))](t)=P[NameList](t);
		OUTPUT:=subs(Subber1,OUTPUT):

		
		Subber1:=P[List2Name(Mat2List(FlipDimension(MatList,1)))](t)=P[NameList](t);
		OUTPUT:=subs(Subber1,OUTPUT):	
		end do:
end do;
OUTPUT:=MakeUnique(OUTPUT);


ICs:=[]:
for h from 1 to nops(OutputList) do
	
	if h=1 then 
		ICs:=[op(ICs),P[List2Name(OutputList[h])](0)=1];
	else
		ICs:=[op(ICs),P[List2Name(OutputList[h])](0)=0];
	end if:
end do:

evalb(nops(OUTPUT)=nops(ICs)),evalb(nops(OUTPUT)=nops(OutputList)),nops(OutputList),OUTPUT,ICs,OutputList,subs(P=Q,OUTPUT),subs(P=Q,ICs);

end proc:</EC-CodeEditor></CodeEditor-ExecGroup>
