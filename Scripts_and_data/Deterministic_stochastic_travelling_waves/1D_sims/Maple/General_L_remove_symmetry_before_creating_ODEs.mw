</Group><CodeEditor-ExecGroup view="presentation" labelreference="L631" drawlabel="true" applyint="true" applyrational="true" display="code"><EC-CodeEditor id="CodeEditRegion2" expanded="true" visible="true" pixel-width="500" pixel-height="200" code-language="text/maple" autofit="true" wrapping="true" show-border="true" code-line-numbers="true">####Loop over possible binary sequences keep only those with correct ICs####
BinaryList:=proc(L,W)
local i, ii, j, B, S, up, down, right, Candidate, checker, TruthList,ToCheck, Checked;
B:=[]:
for i from 0 to 2^(L*W)-1 do
	Candidate:=Binaryiser(i,L*W-1):

	TruthList:=[]:
	for ii from 1 to L do:
		TruthList:=[op(TruthList),evalb(parse(Candidate[1+(ii-1)*W])=1)];
	end do:

	if not member(false,TruthList) then
		S:=convert(List2Name(Candidate), `+`);
		
		ToCheck:=[1];
		Checked:=[];
		
		for j from 1 to L*W while nops(ToCheck)&gt;0 do
					
			up:=ToCheck[1]-W mod L*W: if up=0 then up:=L*W: end if:
			down:=ToCheck[1]+W mod L*W: if down=0 then down:=L*W: end if:
			right:=ToCheck[1]+1 mod L*W: if right=0 then right:=L*W: end if:
			
			if not (member(up,Checked) or member(up,ToCheck)) and (parse(Candidate[up])=1) then
				ToCheck:=[op(ToCheck),up];
			end if:
			
			
			if not (member(down,Checked) or member(down,ToCheck)) and (parse(Candidate[down])=1) then
				ToCheck:=[op(ToCheck),down];
			end if:
			
			
			if not (member(right,Checked) or member(right,ToCheck)) and (parse(Candidate[right])=1) then
				ToCheck:=[op(ToCheck),right];
			end if:
	
			Checked:=[op(Checked),ToCheck[1]];
			ToCheck:=ToCheck[2..-1];
		end do:
	
		if nops(Checked)=S then
			B:=[op(B),Candidate]:
		end if;
		
	end if;
end do:
[B,nops(B),2^(L*W)];
end proc:
</EC-CodeEditor></CodeEditor-ExecGroup>
